{
    "name": "darkghosthunter/preloader",
    "type": "library",
    "description": "Preloader helper to create a PHP-ready preload script from Opcache.",
    "keywords": [
        "darkghosthunter",
        "preloader",
        "php-7.4",
        "opcache"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "homepage": "https://github.com/darkghosthunter/preloader",
    "authors": [
        {
            "name": "Italo Israel Baeza Cabrera",
            "email": "darkghosthunter@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "symfony/finder": "^4.3||^5.0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "DarkGhostHunter\\Preloader\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\" : "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
